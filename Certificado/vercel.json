{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "functions": {
        "server/index.js": {
            "runtime": "vercel-node@latest"
        }
    }
}